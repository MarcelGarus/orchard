{
  "name": "plum",
  "displayName": "Plum",
  "description": "Plum language support",
  "publisher": "MarcelGarus",
  "version": "0.0.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/MarcelGarus/orchard.git",
    "directory": "vscode-extension"
  },
  "//": "https://github.com/ewanharris/vscode-versions",
  "engines": {
    "node": "^18.17.1",
    "vscode": "^1.86.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "workspaceContains:**/*.grass",
    "workspaceContains:**/*.mar",
    "workspaceContains:**/*.mango",
    "workspaceContains:**/*.plum"
  ],
  "main": "./extension.js",
  "contributes": {
    "grammars": [
      {
        "language": "grass",
        "scopeName": "source.grass",
        "path": "./grass.tmGrammar.json"
      },
      {
        "language": "martinaise",
        "scopeName": "source.martinaise",
        "path": "./martinaise.tmGrammar.json"
      },
      {
        "language": "mango",
        "scopeName": "source.mango",
        "path": "./mango.tmGrammar.json"
      },
      {
        "language": "plum",
        "scopeName": "source.plum",
        "path": "./plum.tmGrammar.json"
      }
    ],
    "configurationDefaults": {
      "[grass]": {
        "editor.detectIndentation": false,
        "editor.insertSpaces": true,
        "editor.suggest.insertMode": "replace",
        "editor.tabSize": 2,
        "files.eol": "\n"
      },
      "[martinaise]": {
        "editor.detectIndentation": false,
        "editor.insertSpaces": true,
        "editor.suggest.insertMode": "replace",
        "editor.tabSize": 2,
        "files.eol": "\n"
      },
      "[mango]": {
        "editor.detectIndentation": false,
        "editor.insertSpaces": true,
        "editor.suggest.insertMode": "replace",
        "editor.tabSize": 2,
        "files.eol": "\n"
      },
      "[plum]": {
        "editor.detectIndentation": false,
        "editor.insertSpaces": true,
        "editor.suggest.insertMode": "replace",
        "editor.tabSize": 2,
        "files.eol": "\n"
      }
    },
    "languages": [
      {
        "id": "grass",
        "extensions": [
          ".grass"
        ],
        "aliases": [
          "grass"
        ],
        "configuration": "./grass.config.json"
      },
      {
        "id": "martinaise",
        "extensions": [
          ".mar"
        ],
        "aliases": [
          "martinaise"
        ],
        "configuration": "./martinaise.config.json"
      },
      {
        "id": "mango",
        "extensions": [
          ".mango"
        ],
        "aliases": [
          "mango"
        ],
        "configuration": "./mango.config.json"
      },
      {
        "id": "plum",
        "extensions": [
          ".plum"
        ],
        "aliases": [
          "plum"
        ],
        "configuration": "./plum.config.json"
      }
    ]
  }
}
