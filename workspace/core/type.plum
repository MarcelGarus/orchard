
# Types  

export TypeInfo
export Field
export Variant
export type_of
export type_info

TypeInfo =
  | Int
    String
    Struct (List Field)
    Enum (List Variant)
    Type
Field =
  & name: String
    type: Type
Variant =
  & name: String
    args: List Type

type_of value: t -> Type = builtin_type_of value

type_info type: Type -> TypeInfo = builtin_type_info type
